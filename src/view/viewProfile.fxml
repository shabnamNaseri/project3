<AnchorPane prefHeight="303.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewProfileController">
    <children>
        <Group fx:id="group" layoutX="32.0" layoutY="62.0">
            <children>
                <Label fx:id="nameLBL" layoutY="2.0" prefHeight="33.0" prefWidth="74.0" text="Name :">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
                <Label fx:id="BioLBL" layoutY="49.0" prefHeight="33.0" prefWidth="60.0" text="Bio :">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
                <TextArea fx:id="nameText" layoutX="74.0" prefHeight="17.0" prefWidth="245.0" promptText="name" />
                <TextArea fx:id="bioText" layoutX="74.0" layoutY="47.0" prefHeight="38.0" prefWidth="245.0" promptText="bio" />
                <Button fx:id="exitBTN" layoutX="228.0" layoutY="154.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="92.0" text="Exit" />
                <Label fx:id="editLBL" alignment="CENTER" layoutX="14.0" layoutY="129.0" prefHeight="35.0" prefWidth="187.0" text="Label" textFill="#208a27" />
                <Button fx:id="EditBTN" layoutX="228.0" layoutY="114.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="92.0" text="Edit" />
